{
  "npm.binPath": "$HOME/.local/share/pnpm",
  "suggest.noselect": true,
  "coc.preferences.formatOnSave": true,
  "tailwindCSS.enable": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.suggestions": true,
  "tailwindCSS.validate": true,
  "tailwindCSS.lint.cssConflict": "warning",
  "tailwindCSS.headwind.runOnSave": false,
  "tailwindCSS.headwind.removeDuplicates": true,
  "tailwindCSS.lint.invalidScreen": "warning",
  "tailwindCSS.includeLanguages": { "handlebars": "html" },
  "cSpell.words": [
    "firestore",
    "lucide",
    "alphanum",
    "jotai",
    "semibold",
    "hookform",
    "fastcsv",
    "birthdate",
    "qrSvgs",
    "svgs",
    "jspdf",
    "qrcode",
    "vercel",
    "bkup",
    "cloudinary",
    "dockerhub",
    "meilisearch",
    "dompdf"
  ],
  "suggest.autoTrigger": "none",
  "cSpell.userWords": ["serviceusage"],
	"python.formatting.provider":"None"
}
